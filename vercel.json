{
  "version": 3,
  "functions": {
    "api/**/*.js": {
      "runtime": "vercel-node@1.0.0"
    }
  },
  "build": {
    "env": {},
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "outputDirectory": "build"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
